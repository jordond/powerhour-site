{
  "name": "powerhour-site",
  "version": "0.0.1",
  "description": "Display website for Power Hour for Android",
  "main": "./bin/server.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jordond/powerhour-site.git"
  },
  "author": "Jordon de Hoog",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordond/powerhour-site/issues"
  },
  "homepage": "https://github.com/jordond/powerhour-site#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint": "eslint -c .eslintrc src",
    "start": "better-npm-run start:prod",
    "start-dev": "better-npm-run start:dev",
    "build": "npm run lint && better-npm-run build:prod",
    "build-watch": "npm run lint && better-npm-run build:prod:watch",
    "build-dev": "better-npm-run build:dev",
    "dev": "concurrently --kill-others \"npm run build-dev\" \"npm run start-dev\"",
    "dev-prod": "concurrently --kill-others \"npm run build-watch\" \"npm run start\"",
    "hook-install": "precommit install",
    "hook-remove": "precommit remove",
    "upgrade": "git pull && npm install"
  },
  "precommit": [
    "npm run lint"
  ],
  "betterScripts": {
    "start:prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "./src",
        "PORT": 8080
      }
    },
    "start:dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "./src",
        "PORT": 9000
      }
    },
    "build:prod": {
      "command": "webpack --verbose --colors --display-error-details --config prod.webpack.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:prod:watch": {
      "command": "webpack --watch --debug --verbose --colors --display-error-details --config prod.webpack.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dev": {
      "command": "webpack --watch --debug --colors --display-error-details --config dev.webpack.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 9000
      }
    }
  },
  "dependencies": {
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "pretty-error": "^2.0.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-aria": "^1.5.0",
    "angular-material": "^1.0.6",
    "angular-messages": "^1.5.0",
    "angular-moment": "^1.2.0",
    "angular-ui-router": "^0.4.3",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.6.5",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-runtime": "^6.6.1",
    "better-npm-run": "0.1.0",
    "browser-sync": "^2.11.1",
    "browser-sync-webpack-plugin": "^1.0.1",
    "clean-webpack-plugin": "^0.1.8",
    "compression": "^1.6.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.5.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.9.0",
    "moment": "^2.12.0",
    "ng-annotate": "^1.2.1",
    "ng-annotate-loader": "^0.6.1",
    "ngtemplate-loader": "^2.0.1",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "precommit": "^1.2.0",
    "sass-loader": "^6.0.6",
    "sass-material-colors": "0.0.5",
    "skeleton-css": "^2.0.4",
    "source-map-support": "^0.5.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0"
  }
}

{
  "name": "powerhour-site",
  "version": "0.0.1",
  "description": "Display website for Power Hour for Android",
  "main": "./bin/server.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jordond/powerhour-site.git"
  },
  "author": "Jordon de Hoog",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordond/powerhour-site/issues"
  },
  "homepage": "https://github.com/jordond/powerhour-site#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint": "eslint -c .eslintrc src",
    "start": "better-npm-run start:prod",
    "start-dev": "better-npm-run start:dev",
    "build": "npm run lint && better-npm-run build:prod",
    "build-dev": "better-npm-run build:dev",
    "postinstall": "npm run build",
    "dev": "concurrently --kill-others \"npm run build-dev\" \"npm run start-dev\"",
    "hook-install": "precommit install",
    "hook-remove": "precommit remove"
  },
  "precommit": [
    "npm run lint"
  ],
  "betterScripts": {
    "start:prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "./src",
        "PORT": 8080
      }
    },
    "start:dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "./src",
        "PORT": 9000
      }
    },
    "build:prod": {
      "command": "webpack --verbose --colors --display-error-details --config prod.webpack.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dev": {
      "command": "webpack --watch --debug --colors --display-error-details --config dev.webpack.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 9000
      }
    }
  },
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-aria": "^1.5.0",
    "angular-material": "^1.0.6",
    "angular-messages": "^1.5.0",
    "angular-ui-router": "^0.2.18",
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "font-awesome": "^4.5.0",
    "morgan": "^1.7.0",
    "pretty-error": "^2.0.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-runtime": "^6.6.1",
    "better-npm-run": "0.0.8",
    "browser-sync": "^2.11.1",
    "browser-sync-webpack-plugin": "^1.0.1",
    "clean-webpack-plugin": "^0.1.8",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.0.3",
    "eslint-plugin-react": "^4.2.0",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.9.0",
    "ng-annotate": "^1.2.1",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.4.2",
    "precommit": "^1.2.0",
    "sass-loader": "^3.1.2",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14"
  }
}
